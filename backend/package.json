{
  "name": "qiyal-ai-backend",
  "version": "1.0.0",
  "description": "Backend for Qiyal.ai - AI-powered freelance platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "ts-node src/prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch --runInBand",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "ai",
    "freelance",
    "graphql",
    "nodejs",
    "typescript"
  ],
  "author": "Kakharman Imenov",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@pinecone-database/pinecone": "^6.1.2",
    "@prisma/client": "^6.16.2",
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.6",
    "openai": "^5.23.1",
    "stripe": "^18.5.0",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.14",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/nodemailer": "^7.0.1",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prisma": "^6.16.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
